{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            //"program": "${workspaceFolder}/User.Identity/bin/Debug/netcoreapp3.1/User.Identity.dll",
            //"program": "${workspaceFolder}/Contact.API/bin/Debug/netcoreapp3.1/Contact.API.dll",
            //"program": "${workspaceFolder}/User.Api/bin/Debug/netcoreapp3.1/User.Api.dll",
            "program": "${workspaceFolder}/Project/Project.API/bin/Debug/netcoreapp3.1/Project.API.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Project/Project.API",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}